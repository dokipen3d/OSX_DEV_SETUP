    #!/bin/bash  
      
    echo -e "Installing all .pkg files withing this folder"  
    FILES=*.pkg  
    for f in $FILES  
    do  
              if [ ! -f "$f" ]; then  
                        echo -e "ERROR: no .pkg files found. Quitting..."  
                        exit 1  
              fi  
      
              echo;echo;echo  
              echo "--------------------------------------------------------------------------------"  
              echo "installing $f"  
              echo  
              sudo installer -pkg $f -target /  
              echo "--------------------------------------------------------------------------------"  
    done  
      
    echo -e "\n\nDone!"  